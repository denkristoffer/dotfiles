[user]
    name = Kristoffer
    email = hello@kristoffer.is

[core]
    excludesfile = ~/.gitignore

[credential]
    helper = osxkeychain

[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true
[push]
	default = simple
[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
