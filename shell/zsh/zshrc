# oh-my-zsh
export ZSH="$HOME/.oh-my-zsh"
# $ZSH/themes/ and $ZSH/plugins/
ZSH_THEME="denkristoffer"
plugins=(git yarn history-substring-search thefuck)

. $ZSH/oh-my-zsh.sh

autoload -U compinit; compinit -w -d ~/.config/zsh/cache/zcompdump

# Load z
. `brew --prefix`/etc/profile.d/z.sh

# fnm
eval "$(fnm env)"

# thefuck
eval $(thefuck --alias fuck)

# Contentful k8s
export KUBECONFIG=~/.kube/cf-authinfo.yaml:~/.kube/cf-staging.yaml:~/.kube/cf-preview.yaml:~/.kube/cf-tools.yaml:~/.kube/cf-production.yaml:~/.kube/cf-auth-okta.yaml
